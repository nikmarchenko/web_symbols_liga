@font-face { 
  font-family: 'WebSymbolsLigaRegular';
    src: url(<%= asset_path 'websymbolsligaregular.eot' %>);
    src: url(<%= asset_path 'websymbolsligaregular.eot?#iefix' %>) format('embedded-opentype'),
      url(<%= asset_path 'websymbolsligaregular.woff' %>) format('woff'),
      url(<%= asset_path 'websymbolsligaregular.ttf' %>) format('truetype'),
      url(<%= asset_path 'websymbolsligaregular.svg#WebSymbolsRegular' %>) format('svg');
}



.web_symbol { font-family: 'WebSymbolsLigaRegular'; text-rendering: optimizeLegibility; }